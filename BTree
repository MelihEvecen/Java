class Node {
    int data;
    Node left;
    Node right;

    Node(int data) {
        this.data = data;
        left = null;
        right = null;
    }
}

class BTree {
    Node root;

    BTree() {
        root = null;
    }

    Node newNode(int data) {
        root = new Node(data);
        System.out.println("Node added");
        return root;
    }

    Node insert(Node root, int data) {
        if (root != null) {
            if (data < root.data)
                root.left = insert(root.left, data);
            else
                root.right = insert(root.right, data);
        } else
            root = newNode(data);
        return root;
    }
}

class Main {
    public static void main(String[] args) {
        BTree bt = new BTree();
        bt.root = bt.insert(bt.root, 10);
    }
}
